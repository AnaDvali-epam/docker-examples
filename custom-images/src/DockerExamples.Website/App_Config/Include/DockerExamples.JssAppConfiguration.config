<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<sites>
			<site name="jsssite"
					rootPath="/sitecore/content/JssApp"
					startItem="/home"
					language="en"
					database="web"
					virtualFolder="/"
					physicalFolder="/"
					enableTracking="true"
					cacheHtml="true"
					htmlCacheSize="50MB"
					enablePreview="true"
					enableWebEdit="true"
					enableDebugger="true"
					disableClientData="false" />
		</sites>

		<javaScriptServices>
			<apps>
				<app name="JssApp"
					 sitecorePath="/sitecore/content/JssApp"
					 inherits="defaults"
        />
			</apps>
		</javaScriptServices>
	</sitecore>
</configuration>
